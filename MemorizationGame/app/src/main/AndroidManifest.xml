<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.memorizationgame">
    

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MemorizationGame">
        <service
            android:name=".Business.MusicService"
            android:enabled="true"
            android:exported="true"></service>

        <activity
            android:name=".UI.Game.ResultLv2Activity"
            android:exported="false" />
        <activity
            android:name=".UI.Game.AnswerLv2Activity"
            android:exported="false" />
        <activity
            android:name=".UI.Game.GameLv2Activity"
            android:exported="false" />
        <activity
            android:name=".UI.Game.ResultActivity"
            android:exported="false" />
        <activity
            android:name=".UI.Game.AnswerActivity"
            android:exported="false" />
        <activity
            android:name=".UI.Game.GameActivity"
            android:exported="false" />
        <activity
            android:name=".UI.Login.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".UI.Account.AccountActivity"
            android:exported="false" />
        <activity
            android:name=".ui.login.CreateAccountActivity"
            android:exported="false" />
        <activity
            android:name=".UI.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>